Status InitStack(SqStack &S){
  		S.base[0] = 0;
  		S.top[0] = S.base[0];
  		S.base[1] = stacksize - 1;
  		s.top[1] = S.base[1];
  		return OK;
 }
 Status Push(SqStack &S,int i,SElementType x){
 		if (S.top[0] >= S.top[1])
 			reture ERROR;
 		if (i==0)
 			*S.top[i]++ = x;   
 		else 
 			*S.top[i]-- = x;  
 		 return OK;
 }
 